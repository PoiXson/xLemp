{
	"name":        "pxn/xlemp",
	"description": "Management system for LEMP web servers (Linux / Nginx / MySQL / PHP)",
	"version":     "0.1.0",
	"type":        "project",
	"homepage":    "http://poixson.com/",
	"license":     "GPL-3",
	"support": {
		"issues": "https://github.com/PoiXson/xLemp/issues",
		"source": "https://github.com/PoiXson/xLemp",
		"irc":    "irc://irc.esper.net/poixson"
	},
	"authors": [{
		"name":  "lorenzop",
		"role":  "Developer"
	}],
	"autoload": {
		"psr-4": {
			"pxn\\xLemp\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"pxn\\xLemp\\tests\\":    "tests/",
			"pxn\\xLemp\\examples\\": "examples/"
		}
	},
	"archive": {
		"exclude": [
			"tests/",
			"example/"
		]
	},
	"require": {
		"php":             "5.6.*",
		"pxn/phputils":    "^0.1.2",
		"symfony/console": "^2.7.1"
	},
	"require-dev": {
		"phpunit/phpunit": "^4.7.5"
	}
}
